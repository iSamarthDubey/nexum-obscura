{
  "version": 2,
  "name": "nexum-obscura-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://nexum-obscura-backend.onrender.com/api",
    "REACT_APP_ENV": "production"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://nexum-obscura-backend.onrender.com/api",
      "REACT_APP_ENV": "production"
    }
  }
}
